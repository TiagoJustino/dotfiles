#!/bin/sh

cat \
  | sed "s/\(.*\"timestamp\":[^0-9]*\)\([0-9]*\)[0-9][0-9][0-9]\([^0-9].*\)/echo '\1''\"'\`date -u +'%Y-%m-%d %H:%M:%S' -d @\2\`'\"''\3'/e" \
  | sed "s/\(.*\"ingestionTime\":[^0-9]*\)\([0-9]*\)[0-9][0-9][0-9]\([^0-9].*\)/echo '\1''\"'\`date -u +'%Y-%m-%d %H:%M:%S' -d @\2\`'\"''\3'/e" \
  | sed "s/\\\t/\t/g" \
  | sed "s/\\\n\"/\"/g" \
  | sed "s/\\\n/\n/g"

